num1 = 78
num2 = 95

def funcao_multiplicar(num1, num2):
    print(num1 * num2)
    
funcao_multiplicar(num1, num2)

num1 = 4
num2 = 2

def funcao_dividir(num1, num2):
    print(num1 / num2)
    
funcao_dividir(num1, num2) 

num1 = 6
num2 = 3

def funcao_subtrair(num1, num2):
    print(num1 - num2)
    
funcao_subtrair(num1, num2) 
